target C {
    timeout: 100 ms,
    platform: {
        name: "FlexPRET",
    }
};

reactor PrintHi {
    state x: int = 0
    timer t(0 ms, 10 ms)
    
    reaction(t) {=
        lf_print("Hi with x = %i", self->x++);
    =}
}

main reactor {
    reaction(startup) {=
        lf_print("Hello world (startup)");
    =}

    his = new[3] PrintHi()
}
